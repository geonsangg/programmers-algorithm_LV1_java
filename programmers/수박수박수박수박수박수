// [문제 링크]: https://school.programmers.co.kr/learn/courses/30/lessons/12922

class Solution {
    public String solution(int n) {
        String answer = "";
        if(n%2 == 0) {
            for(int i=1; i<=n/2; i++) {
                answer += "수박";
            }
        } else {
            for(int i=1; i<=n/2; i++) {
                answer += "수박";
            }
            answer += "수";
        }
        return answer;
    }
}